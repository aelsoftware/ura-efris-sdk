{
    "name": "aelsoftware/ura-efris-sdk",
    "description": "A PHP SDK for integrating with the Uganda Revenue Authority (URA) Electronic Fiscal Receipting and Invoicing System (efris) API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "UraEfrisSdk\\": "src/"
        }
    },
    "authors": [
        {
        "name": "Emmanuel Bartile",
        "email": "ebartile@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-openssl": "*",
        "ext-dom": "*",
        "tcdent/php-restclient": "0.1.8.2",
        "ext-curl": "*",
        "symfony/serializer": "v7.0.7",
        "symfony/property-access": "v7.0.7",
        "symfony/property-info": "v7.0.7",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.18"
    },
    "extra": {
        "laravel": {
            "providers": [
                "UraEfrisSdk\\Providers\\SerializerServiceProvider"
            ]
        }
    }
}
